  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "npm run run:server",
    "postinstall": "npm run build:all",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --code-coverage",
    "report-coverage": "codecov -t 1d230cd6-adc8-48d7-87fc-c5f62208cded",
    "test:headless": "karma start karma.conf.js --browsers PhantomJS --autoWatch=true --singleRun=true",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build:server": "tsc -p src/server",
    "build:client": "ng build --prod --output-path=dist/client",
    "build:all": "npm run build:server && npm run build:client",
    "run:server": "node dist/server/app.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/router": "3.4.3",
    "@types/express": "~4.0.34",
    "@types/node": "7.0.0",
    "@types/request-promise": "^4.1.33",
    "@types/socket.io": "1.4.27",
    "@types/socket.io-client": "1.4.29",
    "angular-cli": "1.0.0-beta.25.5",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "intl": "^1.2.5",
