    "rxjs": "5.0.3",
    "serve-static": "^1.11.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "1.7.2",
    "ts-helpers": "1.1.2",
    "ts-node": "2.0.0",
    "tslint": "4.3.1",
    "typescript": "~2.0.3",
    "zone.js": "^0.7.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.3",
